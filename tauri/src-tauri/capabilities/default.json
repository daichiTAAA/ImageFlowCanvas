{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-create",
    "fs:allow-mkdir",
    "fs:allow-read-dir",
    "http:default",
    "core:webview:allow-set-webview-focus",
    "core:window:allow-set-focus",
    "core:webview:allow-print",
    "core:webview:allow-internal-toggle-devtools",
    "dialog:allow-open",
    "dialog:allow-save",
    "shell:allow-open"
  ]
}
